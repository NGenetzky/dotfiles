{
    "editor.minimap.enabled": false,
    "files.insertFinalNewline": true,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "window.zoomLevel": -2,
    // "workbench.colorTheme": "Braver's Solarized Dark (themed workbench)",
    // "workbench.colorTheme": "Solarized Dark",
    ////////////////////////////////////////////////////////////////////////////
    // jira-plugin
    //
    "jira-plugin.workingProject": "VNG",
    "jira-plugin.baseUrl": "https://vaddio.atlassian.net",
    "jira-plugin.username": "ngenetzky",
    // jira-plugin
    //
    ////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////
    // gitlens
    //
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.defaultDateFormat": "YYYY-MM-DDTHH:mm:ss.SSS",
    //
    // gitlens
    ////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////
    // matthias-schwarze.vimter
    //
    // Specifies the path to the terminal version of VIM.
    "VimTer.vimPath": "/usr/bin/vim",
    // matthias-schwarze.vimter
    //
    ////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////
    // ryuta46.multi-command
    //
    "multiCommand.commands": [
        {
            "command": "multiCommand.terminal_tmux_prefix",
            "sequence": [
                "workbench.action.terminal.focus",
                {
                    // This assumes you have tmux running in the integrated
                    // terminal and your prefix is <C-a>.
                    "command": "workbench.action.terminal.sendSequence",
                    "args": {
                        // http://ascii-table.com/ansi-escape-sequences.php
                        "text": "\u0001" // <C-a>
                        // "text": "\u0002" // <C-b>
                    }
                }
            ]
        },
    ],
    //
    // ryuta46.multi-command
    ////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////
    // vscodevim.vim
    //
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    // "vim.insertModeKeyBindings": [
    //     {
    //         "before": [
    //             "j",
    //             "j"
    //         ],
    //         "after": [
    //             "<Esc>"
    //         ]
    //     }
    // ],
    // "vim.normalModeKeyBindingsNonRecursive": [
    //     {
    //         "before": [
    //             "<leader>",
    //             "d"
    //         ],
    //         "after": [
    //             "d",
    //             "d"
    //         ]
    //     },
    // ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-k>": false,
        // "<C-r>": false, // I like redo.
    },
    //
    // vscodevim.vim
    ////////////////////////////////////////////////////////////////////////////
}
