    //     {
    //         "before": [
    //             "<C-n>"
    //         ],
    //         "commands": [
    //             ":nohl"
    //         ]
    //     }
    // ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-k>": false,
    },
    //
    // vscodevim.vim
    ////////////////////////////////////////////////////////////////////////////
}
